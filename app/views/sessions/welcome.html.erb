  <div class="row">
    <h1>Welcome</h1>
    <% if logged_in? %>
    <h1>You are Logged In, <%= current_user.username %></h1>
    <%= button_to "Log Out", '/logout', method: :get%>
    <%end%>
    <% if !logged_in? %>
    <%= button_to "Login", '/login', method: :get%>
    <%= button_to "Sign Up", '/users/new', method: :get%>
    <%end%>
  </div>